{
  "name": "node-express-starter-psql",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.ts",
    "build": "tsc --project ./",
    "dev": "npm-run-all --parallel dev:*",
    "dev:server": "cross-env NODE_ENV=development nodemon server.ts",
    "setup": "npm install && npm run validate",
    "convert": "synp --source-file yarn.lock -f",
    "check-types": "tsc",
    "lint": "eslint --ignore-path .gitignore --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|jsx|ts|tsx|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint build",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy": "is-ci \"test:e2e\" \"cy:open\"",
    "cy:open": "cypress open",
    "cy:run": "cross-env CYPRESS_BASE_URL=http://localhost:5020 cypress run",
    "upgrade-interactive": "npm-check --update",
    "db:makemigrations": "knex migrate:make todo",
    "db:migrate": "knex migrate:latest",
    "db:unmigrate": "knex migrate:rollback",
    "db:makeseed": "knex seed:make todo",
    "db:seed": "knex seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "knex": "^2.4.0",
    "morgan": "^1.10.0",
    "pg": "^8.3.0"
  },
  "devDependencies": {
    "@types/bcrypt-nodejs": "0.0.31",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.7",
    "@types/helmet": "0.0.47",
    "@types/jest": "^26.0.5",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.0.24",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "nodemon": "^2.0.4",
    "npm-check": "^5.9.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "sqlite3": "^5.0.0",
    "ts-jest": "^26.1.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  }
}
